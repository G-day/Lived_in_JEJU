<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.boardtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".WritePostActivity" />
        <activity android:name=".RecuritBoardActivity" />
        <activity android:name=".ApplyBoardActivity" />
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".CommunicateBoardActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".PasswordResetActivity" />
        <activity android:name=".MemberInfoActivity" />

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>